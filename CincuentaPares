package ejercicios_Mats;

public class CincuentaPares {

		public static void main(String[] args) {
			// visualiza los primeros 50 numeros pares
			
			int a=0;
			int i=1;
			
			while (a<=99) {
				
				if ((a%2) == 0) {
					System.out.println(i + ": "+ a);
					i++;
				}

				a++;
				
			}		

	}

}
